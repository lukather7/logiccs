<%= form_for(@branch) do |f| %>
  <%= f.label :name_e, "English branch name", class:"label label-danger width100" %>
  <%= f.text_field :name_e, class:"form-control" %>
  <%= f.label :name_j, "Japanese brabch name", class:"label label-danger width100" %>
  <%= f.text_field :name_j, class:"form-control" %>
  <%= f.submit "Regist Branch", class:"btn btn-primary" %>
<% end %>