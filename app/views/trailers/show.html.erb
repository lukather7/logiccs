
<div class="row">
 <div class="col-md-3">
     <span class="label label-danger width100">Company</span>
　　 <span class="logiccs_table"><%= @trailer.company.name_e if @trailer.company %></span>
  </div>
 <div class="col-md-3">
     <span class="label label-danger width100">Branch</span>
     <span class="logiccs_table"><%= @trailer.branch.name %></span>
 </div>
 <div class="col-md-3">
     <span class="label label-danger width100">Number Plate</span>
     <span class="logiccs_table white"> <%= @trailer.number %></span>
 </div>
 <div class="col-md-3">
     <span class="label label-danger width100">Vehicle Id</span>
     <span class="logiccs_table white"><%= @trailer.vehicleid %></span>
 </div>
</div> 
<hr>
<div class="row"> 
 <div class="col-md-3">
     <span class="label label-danger width100">Wheels</span>
     <span class="logiccs_table white"><%= @trailer.wheels %></span>

  </div>

 <div class="col-md-3">
     <span class="label label-danger width100">Tire</span>
     <span class="logiccs_table white"><%= @trailer.tire %></span>
 </div>



 <div class="col-md-3"><%= link_to "Edit", edit_trailer_path(@trailer), class:"btn btn-primary" %></div>
 <div class="col-md-3">
  <%= link_to "Back to Trailers list", trailers_path, class:"btn btn-primary" %>
 </div>


</div>
<hr>
<div class="row">
 <div class="xol-xs-3">
  <% if @trailer.dealercompany != nil %>

   <div class="col-md-3">
       <span class="label label-danger width100">Dealercompany</span>
       <span class="logiccs_table"><%= @trailer.dealercompany.name %></span>
   </div>
  <% end %>
 </div>
</div>
